<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
           targetNamespace="http://www.sitekick.com"
           xmlns="http://www.sitekick.com"
           elementFormDefault="qualified"
>
    <xs:element name="site">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="url" type="xs:string"></xs:element>
                <xs:element name="globalRank" type="xs:int"></xs:element>
                <xs:element name="country" type="xs:string"></xs:element>
                <xs:element name="countryRank" type="xs:int"></xs:element>
                <xs:element name="category">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="name" type="xs:string" minOccurs="0" ></xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>